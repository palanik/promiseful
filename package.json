{
  "name": "promiseful",
  "version": "0.1.1",
  "description": "Useful patterns with Promise functions",
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb/base"
  },
  "scripts": {
    "lint": "eslint src/**",
    "build": "babel --presets es2015 src -d lib",
    "prepublish": "npm run build",
    "start": "node lib/",
    "dev": "./node_modules/.bin/babel-node src/",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha -- --recursive",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test-mocha": "babel-node ./node_modules/.bin/_mocha --recursive",
    "browser-test": "karma start",
    "browserify": "npm run build && mkdir -p dist && browserify -o dist/promiseful.js -d src/ -t -t babelify --presets es2015 --sourceMapsAbsolute",
    "rollup": "mv .babelrc.rollup .babelrc && rollup -c && rollup -c --environment minify:true && mv .babelrc .babelrc.rollup"
  },
  "keywords": [
    "promise",
    "promises",
    "promiseful",
    "promise functions",
    "control flow",
    "promise flow",
    "resolve",
    "reject",
    "fulfil",
    "async",
    "asynchronous",
    "parallel",
    "series",
    "race"
  ],
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.8.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/palanik/promiseful.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "N. Palani Kumanan",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "isparta": "^4.0.0",
    "karma": "^1.6.0",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "mocha": "^3.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-license": "^0.3.0",
    "rollup-plugin-uglify": "^1.0.1"
  }
}
